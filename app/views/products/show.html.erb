<div class="container-fluid">
  <div class="row">
    <% @product_styles.each do |product_style| %>
    <div class="col-sm-3">
      <div class="card card-block style" id="style-<%=product_style.id%>" >
        <img class="card-img-top" src="<%= product_style.attachment.image.url %>" >
        <div class="card-block">
          <h4 class="card-title"><%= product_style.style.name %></h4>
        </div>
      </div>
    </div>
  <% end %>

  </div>
</div>

